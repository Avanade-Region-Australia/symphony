declare md5=$(printf '%s' "/home/vscode/repos/Terraform-Code" | md5sum | awk '{print $1}')
declare compiledFilePath=~/.lucidity/tmp/$md5

# load dev.compiled.env
dotenv "$compiledFilePath/dev.compiled.env"

# terraform init
path=$(pwd)
key=${path#"/home/vscode/repos/Terraform-Code/terraform/"}
terraform init -backend-config=$compiledFilePath/environments/dev/backend.tfvars -backend-config="key=${key}" -reconfigure 

